public class Solution {
    public boolean checkInclusion(String s1, String s2) {
        if (s1.length() > s2.length()){
            return false;
        }
        int i = 0;
        int m = s1.length();
        int n = s2.length();
        while(i + m <= n){
            if(this.checkAnagram(s1, s2.substring(i, i + m))) return true;
            i += 1;
        }
        return false;
    }

    public boolean checkAnagram(String s1, String s2){
        char[] cs1 = s1.toCharArray();
        char[] cs2 = s2.toCharArray();
        Arrays.sort(cs1);
        Arrays.sort(cs2);
        String s1New = new String(cs1);
        String s2New = new String(cs2);
        if (s1New.equals(s2New)) return true;
        return false;
    }
}
